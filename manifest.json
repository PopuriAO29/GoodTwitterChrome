{
  "name": "GoodTwitter",
  "version": "1.2",
  "description": "Forces your browser to use old twitter.",
  "manifest_version": 2,
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*.twitter.com/",
    "cookies"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}